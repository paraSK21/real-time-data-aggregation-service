{
  "name": "real-time-data-aggregation-service",
  "version": "1.0.0",
  "private": true,
  "description": "Real-time meme coin data aggregation service with REST and WebSocket APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
  "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7",
    "socket.io-client": "^4.7.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
